<table class="table table-hover">
  <thead>
    <tr>
      <th>Title</th>
      <th style="text-align: center">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @loan_member_titles.each do |loan_member_title| %>
      <tr>
        <td><%= loan_member_title.title %></td>
        <td style="text-align: center">
          <%= link_to edit_admins_loan_member_title_path(loan_member_title) do %>
            <i class="icon-pencil3"></i>
          <% end %>
          <%= link_to admins_loan_member_title_path(loan_member_title), method: :delete, data: { confirm: "Are you sure?" } do %>
            <i class="icon-trash-alt color-red"></i>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
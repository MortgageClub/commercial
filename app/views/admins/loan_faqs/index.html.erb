<div class="page-content">
  <div class="content-wrapper">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h5 class="panel-title">Loan FAQs</h5>
        <div class="heading-elements">
          <ul class="icons-list">
            <li>
              <%= link_to new_admins_loan_faq_path do %>
                <i class="icon-plus3"></i>
              <% end %>
            </li>
          </ul>
        </div>
      </div>
      <div class="datatable-scroll">
        <div class="table-responsive">
          <%= render "grid" %>
        </div>
      </div>
      <div class="datatable-footer text-center">
        <%= paginate @loan_faqs, :views_prefix => 'admins' %>
      </div>
    </div>
  </div>
</div>
